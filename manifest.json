{
    "name": "FC Portfolio",
    "description": "Discover people's portfolios and NFT collection data without leaving Farcaster!",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "128": "icons/icon128.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://warpcast.com/*"
            ],
            "js": [
                "scripts/inPage.js"
            ],
            "css": [
                "styles/inPage.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "icons/icon.png"
            ],
            "matches": [
                "https://warpcast.com/*"
            ]
        }
    ]
}